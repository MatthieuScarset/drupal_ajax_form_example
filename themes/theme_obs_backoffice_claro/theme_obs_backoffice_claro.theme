<?php

/**
 * Implements hook_preprocess_HOOK().
 */
function theme_obs_backoffice_claro_preprocess_page(&$variables) {
  $uid = \Drupal::currentUser()->id();
  if ($uid == 1 || $uid == 567) {
    $variables['#attached']['library'][] = 'theme_obs_backoffice_claro/neon';
  }

  $variables['#cache']['contexts'] = [
    'user.is_super_user',
    'user.roles'
  ];

}

