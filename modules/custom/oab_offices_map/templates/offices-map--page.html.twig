<div class="row" >
    <article class="page-offices-map col-md-12 col-sm-12">
       {% if page_title|length > 0 %}<h2>{{ page_title }}</h2>{% endif %}
        <div class="region region-content" >
          <div class="row" >
            <div class='form_filter_regions_countries col-md-12 col-sm-12'>
                {{ regionsCountriesForm }}
            </div>
          </div>
          <div class="row" >
            <div class="block_principal_offices_map col-sm-12">
              <div class="row">
                <div class="block_carte col-lg-9 col-md-8 col-sm-12">
                    {{ mapBlock }}
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12 addresses-list">
                  <div class="row" >
                    <div class="col-lg-12 col-md-12 col-sm-12 labelList">{{ labelList }}</div>
                  </div>
                  <div class="row" >
                    <div class="col-lg-12 col-md-12 col-sm-12">{{ officesListBlock }}</div>
                  </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
    </article>
</div>
